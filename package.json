		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"types": "./src/index.ts"
		}
	},
	"files": [
		"dist",
		"src",
		"package.json",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build": "vite build",
		"publish": "yarn build && yarn npm publish --access public"
	},
	"devDependencies": {
		"typescript": "^5.0.4",
		"vite": "^4.2.1"
	},
	"dependencies": {
		"@michealpearce/utils": "^0.0.14"
	}
}
